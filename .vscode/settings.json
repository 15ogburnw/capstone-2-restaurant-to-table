[
	{
		"diffEditor.codeLens": true,
		"diffEditor.diffAlgorithm": "advanced",
		"diffEditor.experimental.collapseUnchangedRegions": true,
		"diffEditor.experimental.useVersion2": true,
		"editor.autoClosingBrackets": "languageDefined",
		"editor.autoClosingDelete": "always",
		"editor.autoClosingOvertype": "never"
	},

	{
		"editor.foldingStrategy": "auto",
		"editor.defaultFoldingRangeProvider": "zokugun.explicit-folding",
		"editor.acceptSuggestionOnEnter": "off",
		"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
		"editor.colorDecoratorsLimit": 750,
		"editor.cursorSurroundingLines": 7,
		"editor.cursorWidth": 2,
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.definitionLinkOpensInPeek": true,
		"editor.formatOnPaste": true,
		"editor.formatOnSave": false,
		"editor.formatOnSaveMode": "file",
		"editor.formatOnType": true,
		"editor.guides.bracketPairs": true,
		"editor.guides.bracketPairsHorizontal": true,
		"editor.hover.delay": 300,
		"editor.language.brackets": "esbenp.prettier-vscode",
		"editor.linkedEditing": true,
		"editor.minimap.autohide": true,
		"editor.minimap.enabled": false,
		"editor.quickSuggestions": {
			"other": "on",
			"comments": "on",
			"strings": "on"
		},
		"editor.quickSuggestionsDelay": 25,
		"editor.smoothScrolling": true,
		"editor.suggest.insertMode": "replace",
		"editor.suggest.matchOnWordStartOnly": false,
		"editor.suggest.selectionMode": "whenTriggerCharacter",
		"editor.suggest.snippetsPreventQuickSuggestions": true,
		"editor.tabCompletion": "on",
		"editor.tabFocusMode": true,
		"editor.tabSize": 2,
		"editor.wordWrap": "bounded"
	},

	{
		"git.autoStash": true
	},

	{
		"indentRainbow.colors": [
			"rgba(38, 70, 83,0.15)",
			"rgba(42, 157, 143,0.15)",
			"rgba(233, 196, 106, 0.15)",
			"rgba(244, 162, 97,0.15)",
			"rgba(231, 111, 81,0.15)",
			"rgba(231, 111, 81,0.1)",
			"rgba(231, 111, 81,0.05)",
			"rgba(231, 111, 81,0.025)"
		]
	},

	{
		"[javascript]": {
			"editor.maxTokenizationLineLength": 2500
		}
	},

	{
		"openInGitHub.remote.branch": "main"
	},

	{
		"prettier.bracketSameLine": false,
		"prettier.jsxSingleQuote": false,
		"prettier.singleAttributePerLine": false,
		"prettier.singleQuote": false,
		"prettier.trailingComma": "none",
		"prettier.useTabs": true
	},

	{
		"redhat.telemetry.enabled": true
	},

	{
		"security.workspace.trust.emptyWindow": true,
		"security.workspace.trust.enabled": true,
		"security.workspace.trust.startupPrompt": "once",
		"security.workspace.trust.untrustedFiles": "prompt"
	}

	{
		"scm.alwaysShowActions": false,
		"scm.alwaysShowRepositories": false,
		"scm.defaultViewMode": "list",
		"scm.diffDecorationsGutterVisibility": "hover",
	}

	{
		"remoteHub.logLevel": "debug",
		"remoteHub.richNavigation.enabled": "true",
		"remoteHub.switchToCreatedBranch": "always"
	},

	{
		"tabnine.experimentalAutoImports": true
	},

	{
		"tailwind-fold.foldedText": "...classes",
		"tailwind-fold.unfoldIfLineSelected": true
	},

	{
		"template-string-converter.addBracketsToProps": false,
		"template-string-converter.convertOutermostQuotes": true,
		"template-string-converter.autoRemoveTemplateString": true
	},

	{
		"terminal.integrated.tabFocusMode": false
	},

	{
		"todo-tree.filtering.excludeGlobs": [
			"**/node_modules/*/**",
			"**/.yarn/**",
			"**/.next/**",
			"**/.vscode/**",
			"**/.git/**"
		]
	},

	{
		"todo-tree.general.statusBar": "current file",
		"todo-tree.general.statusBar": "none",
		"todo-tree.general.tags": [
			"FIX",
			"BROKEN",
			"TODO",
			"CREATE",
			"COME BACK",
			"XXX",
			"[ ]",
			"[x]"
		]
	},

	{
		"vscode-edge-devtools.browserFlavor": "Default",
		"vscode-edge-devtools.defaultUrl": "http://localhost:3000",
		"vscode-edge-devtools.port": 3000
	},
	{
		"workbench.commandPalette.experimental.suggestCommands": true,
		"workbench.colorTheme":"One Pro Dark Monokai Darker Theme",
		"workbench.commandPalette.experimental.suggestCommands": true,
		"workbench.editor.enablePreviewFromQuickOpen": false,
		"workbench.editor.highlightModifiedTabs": false,
		"workbench.editor.labelFormat": "medium",
		"workbench.editor.limit.value": "enabled",
		"workbench.editor.limit.value": 10,
		"workbench.editor.tabSizing": "fixed",
		"workbench.editor.tabSizingFixedMaxWidth": 250,
	}
]

