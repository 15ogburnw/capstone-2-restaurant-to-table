	"portsAttributes": {
		"3000":{
			"label": "Application",
			"onAutoForward": "notify",
			"elevateIfNeeded":true
		},
		"otherPortsAttributes": {"onAutoForward": "silent"}
	},

	"forwardPorts": [3000],
		


