// For format details, see https://aka.ms/devcontainer.json. For config options, see the
// README at: https://github.com/devcontainers/templates/tree/main/src/ubuntu
{
	"name": "Ubuntu",
	// Or use a Dockerfile or Docker Compose file. More info: https://containers.dev/guide/dockerfile
	"image": "mcr.microsoft.com/devcontainers/base:jammy",
	"features": {
		"ghcr.io/devcontainers/features/docker-outside-of-docker:1": {},
		"ghcr.io/devcontainers/features/github-cli:1": {},
		"ghcr.io/devcontainers/features/powershell:1": {},
		"ghcr.io/devcontainers/features/sshd:1": {},
		"ghcr.io/rocker-org/devcontainer-features/apt-packages:1": {},
		"ghcr.io/devcontainers-contrib/features/apt-get-packages:1": {},
		"ghcr.io/devcontainers-contrib/features/autoenv:1": {},
		"ghcr.io/devcontainers-contrib/features/bash-command:1": {},
		"ghcr.io/devcontainers-contrib/features/curl-apt-get:1": {},
		"ghcr.io/devcontainers-contrib/features/jest:2": {},
		"ghcr.io/devcontainers-contrib/features/jshint:2": {},
		"ghcr.io/devcontainers-contrib/features/localtunnel-npm:1": {},
		"ghcr.io/devcontainers-contrib/features/npm-package:1": {},
		"ghcr.io/devcontainers-contrib/features/typescript:2": {},
		"ghcr.io/devcontainers-contrib/features/wget-apt-get:1": {},
		"ghcr.io/eliises/devcontainer-features/bash-profile:1": {},
		"ghcr.io/eliises/devcontainer-features/devcontainers-cli:1": {},
		"ghcr.io/akhildevelops/devcontainer-features/apt:0": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"christian-kohler.npm-intellisense",
				"dbaeumer.vscode-eslint",
				"ms-vscode-remote.vscode-remote-extensionpack",
				"ms-vscode.remote-server",
				"ms-vscode.remote-explorer",
				"mhutchie.git-graph",
				"donjayamanne.githistory",
				"codezombiech.gitignore",
				"esbenp.prettier-vscode",
				"christian-kohler.path-intellisense",
				"redhat.vscode-yaml",
				"formulahendry.auto-rename-tag",
				"formulahendry.auto-close-tag",
				"bradlc.vscode-tailwindcss",
				"Gruntfuggly.todo-tree",
				"leizongmin.node-module-intellisense",
				"vincaslt.highlight-matching-tag",
				"HTMLHint.vscode-htmlhint",
				"kisstkondoros.vscode-gutter-preview",
				"SimonSiefke.svg-preview",
				"yatki.vscode-surround",
				"rangav.vscode-thunder-client",
				"VisualStudioExptTeam.vscodeintellicode-completions",
				"arcanis.vscode-zipfs",
				"riazxrazor.html-to-jsx"
			]
		},
		"postCreateCommand": "yarn fresh-install"
	}



	
	
	
	// Configure tool-specific properties.
	// "customizations": {},
	// Uncomment to connect as root instead. More info: https://aka.ms/dev-containers-non-root.
	// "remoteUser": "root"
}
