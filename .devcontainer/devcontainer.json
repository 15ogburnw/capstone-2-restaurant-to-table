				"tailwind-fold.foldedText": "...classes",
				"thunder-client.httpLibrary": "axios",
				"tsimporter.filesToExclude": [
					"**/.git/**",
					"**/.yarn/**",
					"**/.pnp.*/**"
				],
				"remote.SSH.remoteServerListenOnSocket": true,
				"remoteHub.logLevel": "info",
				"remoteHub.richNavigation.enabled": true,
				"tabout.disableByDefault": true,
				"tailwindCSS.emmetCompletions": true,
				"tailwindCSS.experimental.configFile": "../tailwind.config.js",
				"tailwindCSS.files.exclude": [
					"**/.git/**",
					"**/node_modules/**",
					"**/.yarn/**",
					"**/.pnp.*/**"
				],
				"debug.javascript.breakOnConditionalError": true,
				"json.trace.server": "verbose",
				"npm.packageManager": "yarn",
