					"!**/.yarn/**",
					"**.pnp.**"
				],
				"git-graph.commitDetailsView.fileView.fileTree.compactFolders": false,
				"git-graph.dialog.addTag.pushToRemote": true,
				"git-graph.dialog.applyStash.reinstateIndex": true,
				"git-graph.dialog.cherryPick.noCommit": true,
