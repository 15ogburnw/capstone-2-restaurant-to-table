				"prettier.jsxSingleQuote": true,
				"prettier.requireConfig": true,
				"prettier.resolveGlobalModules": true,
				"prettier.singleQuote": true,
				"prettier.useTabs": true,
				"remote.SSH.defaultForwardedPorts": [
					{
					"remotePort": 3000
					}
