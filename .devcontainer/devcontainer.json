{
	"name": "Ubuntu",

	"image": "mcr.microsoft.com/devcontainers/base:ubuntu-jammy",

	"features": {

		"ghcr.io/devcontainers/features/github-cli:1": {},
		"ghcr.io/devcontainers/features/sshd:1": {},
		"ghcr.io/devcontainers-contrib/features/autoenv:1": {},
		"ghcr.io/devcontainers-contrib/features/curl-apt-get:1": {},
		"ghcr.io/devcontainers-contrib/features/jest:2": {},
		"ghcr.io/devcontainers-contrib/features/jshint:2": {},
		"ghcr.io/devcontainers-contrib/features/localtunnel-npm:1": {},
		"ghcr.io/devcontainers-contrib/features/typescript:2": {},
		"ghcr.io/devcontainers-contrib/features/wget-apt-get:1": {}
	},
	"customizations": {

		"codespaces": {
			"openFiles": [
				"pages/index.js"
			]
		},
		"vscode": {
			"settings": {
				"todo-tree.filtering.ignoreGitSubmodules": true,
